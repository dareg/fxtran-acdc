use FindBin qw ($Bin);
use lib "$Bin/../lib";

use strict;

use Fxtran::Common;
use Fxtran::Bt;
use Fxtran::Util;

my $package = 'Fxtran::Generate';

if (@ARGV && ($ARGV[0] =~ m/::/o))
  {
    $package = shift;
  }

&Fxtran::Util::loadModule ($package);

$package->run (@ARGV);


